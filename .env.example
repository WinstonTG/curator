# Database
DATABASE_URL="postgresql://user:password@localhost:5432/curator_dev?schema=public"

# Feature Flags
FEATURE_PRIVACY=true
FEATURE_EMBEDDINGS=false
FEATURE_VECTOR_SEARCH=false
FEATURE_CONTEXT_ENGINE=false
FEATURE_NOVELTY_BOOST=false

# API Keys
ANTHROPIC_API_KEY=your_api_key_here

# External API Keys - Data Ingestion Connectors
# Spotify (https://developer.spotify.com/dashboard)
SPOTIFY_CLIENT_ID=your_spotify_client_id
SPOTIFY_CLIENT_SECRET=your_spotify_client_secret

# News API (https://newsapi.org)
NEWS_API_KEY=your_news_api_key

# YouTube Data API (https://console.cloud.google.com)
YOUTUBE_API_KEY=your_youtube_api_key

# Spoonacular (https://spoonacular.com/food-api)
SPOONACULAR_API_KEY=your_spoonacular_api_key

# Eventbrite (https://www.eventbrite.com/platform)
EVENTBRITE_TOKEN=your_eventbrite_oauth_token

# Server
PORT=3001
NODE_ENV=development
