# Feature Flags
# Set to 'true' to enable features

# Privacy & Data Controls (Export, Delete)
FEATURE_PRIVACY=true
NEXT_PUBLIC_FEATURE_PRIVACY=true

# Vector Search & Embeddings
FEATURE_EMBEDDINGS=false
NEXT_PUBLIC_FEATURE_EMBEDDINGS=false

FEATURE_VECTOR_SEARCH=false
NEXT_PUBLIC_FEATURE_VECTOR_SEARCH=false

# Context Engine (Weather, Time of Day)
FEATURE_CONTEXT_ENGINE=false
NEXT_PUBLIC_FEATURE_CONTEXT_ENGINE=false

# Novelty Boost in Ranking
FEATURE_NOVELTY_BOOST=false
NEXT_PUBLIC_FEATURE_NOVELTY_BOOST=false

# Note: NEXT_PUBLIC_* variables are exposed to the browser
# Use non-prefixed variables for server-side only features
