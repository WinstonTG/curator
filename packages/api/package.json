{
  "name": "@curator/api",
  "version": "0.1.0",
  "description": "Express API server for Curator platform",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "dev": "tsx watch src/index.ts",
    "build": "tsc",
    "start": "node dist/index.js",
